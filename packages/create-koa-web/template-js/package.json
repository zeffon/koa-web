{
  "name": "koa-web-js",
  "version": "0.0.0",
  "description": "The best scaffolding of building Koa2 restful API.",
  "engines": {
    "node": ">=12.2.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "start": "concurrently npm:build&&nodemon build/app.js",
    "build": "babel src -d build",
    "prod": "set NODE_ENV=production&&node build/app.js",
    "test": "set NODE_ENV=test&&jest"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/runtime": "^7.17.7",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.21",
    "log4js": "^6.4.1",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "redis": "3.0.2",
    "sequelize": "^6.17.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime-corejs3": "^7.17.7",
    "concurrently": "^7.0.0",
    "jest": "^27.5.1",
    "koa-swagger-decorator": "^1.8.5",
    "lint-staged": "^12.3.3",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "supertest": "^6.2.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
